{
  "version": "1.0",
  "local_store": "/var/lib/git2consul_cache",
  "logger": {
  "streams": [{
  		"level": "trace",
		"stream": "process.stdout"
  	},
  	{
		"level": "debug",
		"type": "rotating-file",
		"path": "/var/log/git2consul/git2consul.log"
  	}]
  },
  "repos" : [{
  	"name" : "netfilter_config_name_prefix",
	"url" : "https://github.com/numenshane/netfilter.git",
	"support_tags": true,
	"source_root": "nodejs-demo/wd_service_spec",
	"branches" : ["master"],
	"hooks": [{
		"type" : "polling",
		"interval" : "1"
    }]
  }]
}
